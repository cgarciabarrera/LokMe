





<% if user_signed_in? %>




    <h1>Listing characters</h1>

    <table>
      <tr>
        <th>Name</th>
        <th>Address</th>
        <th>Longitude</th>
        <th>Latitude</th>
        <th>Gmaps</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>

      <% @devices.each do |character| %>
          <tr>
            <td><%= character.imei %></td>
            <td><%= character.name %></td>
            <td><%= character.longitude %></td>
            <td><%= character.latitude %></td>
            <td><%= character.gmaps %></td>

          </tr>
      <% end %>
    </table>

    <br />
<%= link_to 'New Device', new_device_path %>
<%= gmaps4rails(@json) %>
<% end %>